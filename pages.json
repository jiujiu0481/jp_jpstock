			"path": "pages/market/overview", // 市场概况
			"style": {
				"enablePullDownRefresh": true,
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/stock/overview", // 单股概况
			"style": {
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/stock/buy", // 单股买入
			"style": {
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/trade/day/index", // 日内交易
			"style": {
