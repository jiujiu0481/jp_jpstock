			}
		}, {
			"path": "pages/trade/issuance/record", //  新股配售 記錄[申請|中簽]
			"style": {
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/trade/ipo/index", // ipo交易
			"style": {
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/trade/ipo/record", // ipo交易 記錄[申請|中簽]
			"style": {
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/trade/large/index", // 大宗交易
			"style": {
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/trade/large/record", // 日内交易 記錄[申請|審批]
			"style": {
				"enablePullDownRefresh": false,
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/position/index", // 持仓
			"style": {
				"navigationBarTitleText": "注文·照会",
				"enablePullDownRefresh": true,
				"navigationStyle": "custom"
				// "onReachBottomDistance": 1000 // 触发时，距底距离。
			}
		}, {
			"path": "pages/position/info", // 持仓
			"style": {
				"enablePullDownRefresh": true,
				"navigationStyle": "custom"
				// "onReachBottomDistance": 1000 // 触发时，距底距离。
			}
		}
	],

	"condition": {
		//模式配置，仅开发期间生效
		"current": 0, //当前激活的模式(list 的索引项)
		"list": [{
			"name": "", //模式이름
			"path": "", //启动页面，必选
			"query": "" //启动参数，在页面的onLoad函数里面得到
		}]
	}
